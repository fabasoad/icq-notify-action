  "devDependencies": {
    "@types/jest": "29.2.3",
    "@types/node": "18.11.9",
    "@types/sinon": "10.0.13",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "@vercel/ncc": "0.34.0",
    "chai": "4.3.7",
    "eslint": "8.28.0",
    "eslint-config-google": "0.14.0",
    "icq-bot": "2.1.45",
    "jest": "29.3.1",
    "jest-circus": "29.3.1",
    "mocha-param": "2.0.1",
    "sinon": "14.0.2",
    "ts-jest": "29.0.3",
    "typescript": "4.9.3"
  }
