{
  "name": "icq-notify-action",
  "version": "1.1.2",
  "description": "ICQ Notify GitHub Action",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:fabasoad/icq-notify-action.git"
  },
  "scripts": {
    "build": "ncc build -m src/index.ts",
    "lint:md": "markdownlint --config .markdownlint.yml '**/*.md'",
    "lint:es": "eslint --ext ts src",
    "lint": "yarn run lint:md && yarn run lint:es",
    "postinstall": "yarn husky install",
    "prepare": "yarn run snyk-protect",
    "security:auth": "snyk auth",
    "security:test": "snyk test",
    "snyk-protect": "snyk protect",
    "test": "jest --config=jest.config.json --coverage"
  },
  "keywords": [
    "actions",
    "github-actions"
  ],
  "author": "Yevhen Fabizhevskyi",
  "license": "MIT",
  "homepage": "https://github.com/fabasoad/icq-notify-action#readme",
  "dependencies": {
    "@actions/core": "1.2.7",
    "@actions/tool-cache": "1.6.1",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/jest": "26.0.23",
    "@types/node": "15.6.1",
    "@types/sinon": "10.0.0",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "@vercel/ncc": "0.28.6",
    "chai": "4.3.4",
    "eslint": "7.25.0",
    "eslint-config-google": "0.14.0",
    "git-branch-is": "4.0.0",
    "husky": "6.0.0",
    "icq-bot": "2.1.44",
    "jest": "26.6.3",
    "jest-circus": "26.6.3",
    "markdownlint-cli": "0.27.1",
    "mocha-param": "2.0.1",
    "sinon": "10.0.1",
    "snyk": "1.574.0",
    "ts-jest": "26.5.5",
    "typescript": "4.3.2"
  },
  "snyk": true
}
