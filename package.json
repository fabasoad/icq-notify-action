{
  "name": "icq-notify-action",
  "version": "1.1.6",
  "description": "ICQ Notify GitHub Action",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:fabasoad/icq-notify-action.git"
  },
  "scripts": {
    "build": "ncc build -m src/index.ts",
    "lint:md": "markdownlint --config .markdownlint.yml '**/*.md'",
    "lint:es": "eslint --ext ts src",
    "lint": "yarn run lint:md && yarn run lint:es",
    "postinstall": "yarn husky install",
    "prepare": "yarn run snyk-protect",
    "security:auth": "snyk auth",
    "security:test": "snyk test",
    "snyk-protect": "snyk protect",
    "test": "jest --config=jest.config.json --coverage"
  },
  "keywords": [
    "actions",
    "github-actions"
  ],
  "author": "Yevhen Fabizhevskyi",
  "license": "MIT",
  "homepage": "https://github.com/fabasoad/icq-notify-action#readme",
  "dependencies": {
    "@actions/core": "1.9.1",
    "@actions/tool-cache": "1.7.1",
    "winston": "3.6.0"
  },
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@types/node": "17.0.21",
    "@types/sinon": "10.0.11",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "@vercel/ncc": "0.33.3",
    "chai": "4.3.6",
    "eslint": "8.10.0",
    "eslint-config-google": "0.14.0",
    "git-branch-is": "4.0.0",
    "husky": "7.0.4",
    "icq-bot": "2.1.45",
    "jest": "27.5.1",
    "jest-circus": "27.5.1",
    "markdownlint-cli": "0.31.1",
    "mocha-param": "2.0.1",
    "sinon": "13.0.1",
    "snyk": "1.860.0",
    "ts-jest": "27.1.3",
    "typescript": "4.6.2"
  },
  "snyk": true
}
